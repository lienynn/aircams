<%= simple_form_for(@camera) do |f| %>
  <%= f.label :model %>
  <%= f.text_field :model %>
  <%= f.label :address %>
  <%= f.text_field :address %>
    <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.label :price_per_day %>
  <%= f.number_field :price_per_day %>
  <%= f.input :photo, as: :file %>
   <%# Will need to add picture %>
  <%= f.submit "Update!", class: "btn btn-success"%>
<% end %>
